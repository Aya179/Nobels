<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <title>NOBLES</title>

    <!-- Custom fonts for this template-->
    <link href="~/vendor/fontawesome-free/css/all.min.css"
          rel="stylesheet"
          type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    <!-- Custom styles for this template-->
    <link href="~/css/sb-admin-2.css" rel="stylesheet" />


    <!-- Custom styles for this page -->
    <link href="~/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
</head>

<body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion"
            id="accordionSidebar">
            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center"
               href="index.html">
                <img src="~/vendor/Artboard_4_300x.png" alt="" width="200" height="60">
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0" />

            <!-- Nav Item - Dashboard -->
            @if (ViewContext.RouteData.Values["action"].Equals("home"))
            {
                <li class="nav-item  active">
                    <a class="nav-link" asp-controller="showRoomAndProjectManger" asp-action="home">
                        <i class="fas fa-fw fa-tachometer-alt"></i>
                        <span>الصفحة الرئيسية</span>
                    </a>
                </li>

            }
            else
            {
                <li class="nav-item  ">
                    <a class="nav-link" asp-controller="showRoomAndProjectManger" asp-action="home">
                        <i class="fas fa-fw fa-tachometer-alt"></i>
                        <span>الصفحة الرئيسية</span>
                    </a>
                </li>
            }


            <!-- Divider -->
            <hr class="sidebar-divider" />

            <!-- Heading -->
            @if (ViewContext.RouteData.Values["action"].Equals("Index"))
            {
                <li class="nav-item  active">
                    <a class="nav-link" asp-controller="showRoomAndProjectManger" asp-action="Index">
                        <i class="fas fa-fw fa-user"></i>
                        <span>العملاء</span>
                    </a>
                </li>

            }
            else
            {
                <li class="nav-item  ">
                    <a class="nav-link" asp-controller="showRoomAndProjectManger" asp-action="Index">
                        <i class="fas fa-fw fa-user"></i>
                        <span>العملاء</span>
                    </a>
                </li>
            }

            <li class="nav-item">
                <a class="nav-link collapsed"
                   href="~/Reports/CityReportView"
                   data-toggle="collapse"
                   data-target="#collapseTwo"
                   aria-expanded="true"
                   aria-controls="collapseTwo">
                    <i class="fas fa-fw fa-cog"></i>
                    <span>التقارير</span>
                </a>
                <div id="collapseTwo"
                     class="collapse"
                     aria-labelledby="headingTwo"
                     data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">


                        @*                            <a class="collapse-item" href="~/AddCustomer/CityReportView">تقرير مبيع حسب المدينة</a>
                        *@


                        @*                            <a class="collapse-item" href="~/AddCustomer/ManegerReportView">تقرير مبيع حسب البائع</a>
                        *@


                        <a class="collapse-item" href="~/showRoomAndProjectManger/CustomerReportView">تقرير مبيع حسب الزبون</a>


                        <a class="collapse-item" href="~/showRoomAndProjectManger/BranchReportView">تقرير مبيعات هذا الفرع</a>
                        <a class="collapse-item" href="~/showRoomAndProjectManger/AllSelesReportView">تقرير مبيعاتي</a>




                    </div>
                </div>
            </li>


            


            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block" />

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>
        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">
                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop"
                            class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>

                    <!-- Topbar Search -->
                   
                    <!-- Topbar Navbar -->
                    <ul class="navbar-nav ml-auto">
                        <!-- Nav Item - Search Dropdown (Visible Only XS) -->
                        <li class="nav-item dropdown no-arrow d-sm-none">
                            <a class="nav-link dropdown-toggle"
                               href="#"
                               id="searchDropdown"
                               role="button"
                               data-toggle="dropdown"
                               aria-haspopup="true"
                               aria-expanded="false">
                                <i class="fas fa-search fa-fw"></i>
                            </a>
                            <!-- Dropdown - Messages -->
                            <div class="dropdown-menu dropdown-menu-right p-3 shadow animated--grow-in"
                                 aria-labelledby="searchDropdown">
                                <form class="form-inline mr-auto w-100 navbar-search">
                                    <div class="input-group">
                                        <input type="text"
                                               class="form-control bg-light border-0 small"
                                               placeholder="Search for..."
                                               aria-label="Search"
                                               aria-describedby="basic-addon2" />
                                        <div class="input-group-append">
                                            <button class="btn btn-primary" type="button">
                                                <i class="fas fa-search fa-sm"></i>
                                            </button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </li>

                        <!-- Nav Item - Alerts -->
                       

                        <!-- Nav Item - Messages -->
                        <li class="nav-item dropdown no-arrow mx-1">
                            <a class="nav-link dropdown-toggle"
                               href="#"
                               id="b"
                               role="button"
                               data-toggle="dropdown"
                               aria-haspopup="true"
                               aria-expanded="false">
                                <span id="bra"></span>-<span id="bra1"></span>
                            </a>
                            <!-- Dropdown - Messages -->
                        </li>

                        <div class="topbar-divider d-none d-sm-block"></div>

                        <!-- Nav Item - User Information -->
                        <partial name="_LoginPartial" />
                        <ul class="navbar-nav flex-grow-1">
                            @*<li class="nav-item">
                            go

                            </li>*@
                        </ul>

                    </ul>
                </nav>
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    @RenderBody()


                </div>
                <!-- /.container-fluid -->
            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; TimberLand 2023</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->
        </div>
        <!-- End of Content Wrapper -->
    </div>
    <!-- End of Page Wrapper -->
    <!-- Bootstrap core JavaScript-->
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <!-- Page level plugins -->
    <script src="~/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/js/demo/chart-area-demo.js"></script>
    <script src="~/js/demo/chart-pie-demo.js"></script>


    <!-- Page level plugins -->
    <script src="~/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/js/demo/datatables-demo.js"></script>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.25.3/moment.min.js"
            integrity="sha512-Z0SCX0bDbIzeUvsuax6apRISiAoxS1mXL05AO0tLdthrLb/uaVYUCqiRzMi/vqip5GQeo2p6jaMplnGl//yOvA=="
            crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script src="~/js/jquery.equal-width-children.js"></script>

    <script src='https://cdn.jsdelivr.net/npm/fullcalendar@6.1.8/index.global.min.js'></script>


    @RenderSection("Scripts", required: false)
    <script>
        var date = new Date(); // Now
        date.setDate(date.getDate() + 30); // Set now + 30 days as the new date

        $(function () {
            $('[type="date"]').prop('max', function () {
                return date.toJSON().split('T')[0];
            });
        });
    </script>
    <script>
        function get_id() {


            $.get("/showRoomAndProjectManger/GetBranchName", function (data) {
                document.getElementById('bra').innerHTML = data;




            });

        }



        function get_id1() {


            $.get("/showRoomAndProjectManger/GetCityName", function (data) {
                document.getElementById('bra1').innerHTML = data;




            });


        }
        $(document).ready(function () {
            get_id();
            get_id1();

        });
    </script>


</body>
</html>

