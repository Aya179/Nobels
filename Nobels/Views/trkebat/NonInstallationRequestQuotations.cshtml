@model IEnumerable<Nobels.Models.productionClass>

@{
    ViewData["Title"] = "Index";
    Layout = "_LayoutRepresntitiveAndAdvisor";

}

<div class="row" style="margin-top:10px;">
    <div class="col-12">


        <div class="card">

            <div class="card-header">
                <h3 class="card-title">
                    العروض غير المرفوعة للتركيب
                </h3>
               
            </div>

            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped" id="dep_table">

                        <thead style="white-space: nowrap;">
                            <tr>
                                @* <th style="text-align: center;">
                                @Html.DisplayNameFor(model => model.Id)
                                </th>*@
                                <th style="text-align: center;">
                                   	رقم العرض
                                </th>
                                <th style="text-align: center;">
                                    تاريخ إنشاء العرض
                                </th>
                                <th style="text-align: center;">
                                    العميل
                                </th>
                                <th style="text-align: center;">
                                    المدينة
                                </th>
                                <th style="text-align: center;">
                                   تاريخ التدقيق الفني
                                </th>
                                <th style="text-align: center;">
                                  تاريخ التدقيق المالي
                                </th>
                                <th style="text-align: center;">
                                   
                                </th>
                             
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    @*<td style="text-align: center;">
                                @Html.DisplayFor(modelItem => item.Id)
                                </td>*@
                                    <td style="text-align: center;">
                                        @Html.DisplayFor(modelItem => item.quotionNumberId)
                                    </td>
                                    <td style="text-align: center;">
                                        @{
                                            var date = item.QuotationSimpleDate.Value.ToShortDateString();
                                        }
                                        @Html.DisplayFor(modelItem => date)
                                    </td>

                                    @* <td style="text-align: center;">
                                @Html.DisplayFor(modelItem => item.SecondAddress)
                                </td>*@
                                    <td style="text-align: center;">
                                        @Html.DisplayFor(modelItem => item.CustomerArabicName)
                                    </td>
                                    <td style="text-align: center;">
                                        @Html.DisplayFor(modelItem => item.city)
                                    </td>
                                    @if (item.TechnicalofficeDate == null)
                                    {<td style="text-align: center;">
                                        
                                       -
                                    </td>
                                        
                                      }
                                     else{
                                        <td style="text-align: center;">

                                            @Html.DisplayFor(modelItem => item.TechnicalofficeDate)
                                        </td>
                                       }
                                    
                                       @if(item.FinancialDate==null){
                                        <td style="text-align: center;">
                                           -
                                        </td>
                                       }
                                       else{
 <td style="text-align: center;">
                                        @Html.DisplayFor(modelItem => item.FinancialDate)
                                    </td>
                                       }
                                   
                                   

                                    <td style="text-align: center; white-space: nowrap;">

                                        <a class="bill5" asp-controller="trkebat" asp-action="trkebat_offer" asp-route-id="@item.quotionNumberId"> رفع طلب تركيب  </a>

                                    </td>

                                </tr>
                            }
                        </tbody>



                    </table>

                </div>
            </div>
        </div>
    </div>

</div>



@section Scripts {
    <script>
        $(function () {
            var DataTables = $('#dep_table').DataTable({
               
                "autoWidth": true,
                "language": {
                    "sProcessing": "جارٍ التحميل...",
                    "sLengthMenu": "أظهر _MENU_ مدخلات",
                    "sZeroRecords": "لم يتم العثور على أية نتائج",
                    "sInfo": "إظهار _START_ إلى _END_ من أصل _TOTAL_ مدخل",
                    "sInfoEmpty": "يعرض 0 إلى 0 من أصل 0 سجل",
                    "sInfoFiltered": "(منتقاة من مجموع _MAX_ مُدخل)",
                    "sInfoPostFix": "",
                    "sSearch": "البحث:",
                    "sUrl": "",
                    "oPaginate": {
                        "sFirst": "الأول",
                        "sPrevious": "السابق",
                        "sNext": "التالي",
                        "sLast": "الأخير"
                    }
                }
            });
           

        });</script>
}
