                        <th>F Cost</th>
                        <th>I Cost</th>*@
                        <th>اللون رقم 1</th>
                        <th>اللون رقم 2</th>
                        <th>اللون رقم 3</th>
                        <th></th>
                    </tr>
                </thead>

                <tbody id="products_body">
                </tbody>



            </table>


            <div class="" style="display: flex;align-items: flex-start;flex-direction: column;">

