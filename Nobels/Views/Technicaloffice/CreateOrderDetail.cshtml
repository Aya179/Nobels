            <input id="itemdes" hidden />
            <input class="form-control" type="text" name="order_num" id="priceinput" hidden>


            <hr />
            <form id="myform">
                <div class="row justify-content-between">

                    <div class="col-3">
                        <label for="sub_cat"> المنتج الأساسي</label>
